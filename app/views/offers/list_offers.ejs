  .badge_ver_avaliacoes{
    display: inline-block;
    min-width: 10px;
    padding: 3px 7px;
    font-size: 12px;
    font-weight: bold;
    line-height: 1.5;
    color: #808080;
    text-align: center;
    /* height: 10px white-space: nowrap; */
    vertical-align: middle;
    background-color: rgba(223, 222, 4, 0.17);
    border-radius: 10px;
  }
  



